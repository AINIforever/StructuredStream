create env spark(
    spark.default.parallelism='2',
    spark.sql.shuffle.partitions='2'
)WITH(
    appname='WooTest'
);

CREATE TABLE InputTable(
    id Int,
    number1 Int
)WITH(
    type='socket',
    host='localhost',
    processwindow='10 seconds,10 seconds',
    watermark='30 seconds',
    port='9998'
);

CREATE SINK OutputTable(
)WITH(
    type='console',
    outputmode='Append',
    trigger='2 seconds'
);

insert into OutputTable select * from InputTable;
