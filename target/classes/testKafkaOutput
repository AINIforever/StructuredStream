create env spark(
    spark.default.parallelism='2',
    spark.sql.shuffle.partitions='2'
)WITH(
    appname='kafkatest'
);

CREATE TABLE inputTable(
    word string
)WITH(
    type='socket',
    processwindow='10 seconds,10 seconds',
    watermark='10 seconds',
    host='localhost',
    port='9998'
);

create SINK kafkaOut(
)WITH(
    type='kafka',
    outputmode='complete',
    process='10s',
    kafka.bootstrap.servers='localhost:9092',
    topic='test'
);

insert into kafkaOut select processwindow,word,count(*) from inputTable group by word,processwindow;