create env spark(
    spark.default.parallelism='2',
    spark.sql.shuffle.partitions='2'
)WITH(
    appname='CsvTest'
);

CREATE TABLE csvTable(
    name string,
    age int
)WITH(
    type='csv',
    delimiter=';',
    path='E:\Woo\2019summerPra\spark1\filepath'
);

create SINK console(
)WITH(
    type='console',
    outputmode='complete',
);

insert into console select name,sum(age) from csvTable group by name;